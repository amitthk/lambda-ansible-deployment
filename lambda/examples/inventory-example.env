# Environment Variables for Lambda Deployment
# Copy this file to .env and fill in your actual values

# AWS Configuration
AWS_ACCESS_KEY_ID=AKIA...
AWS_SECRET_ACCESS_KEY=xyz123...
AWS_DEFAULT_REGION=us-east-1
AWS_S3_BUCKET_REPOSITORY=sampleapp-artifacts

# SIT Environment Configuration
SIT_VM_HOSTNAME=sit-vm-01.example.com
SIT_VM_USERNAME=ec2-user
SIT_VM_SSH_KEY=SIT_VM_SSH_KEY
SIT_DATASOURCE_URL=jdbc:postgresql://sit-db.example.com:5432/sampleapp
SIT_DB_USERNAME=sampleapp_user
SIT_DB_PASSWORD=sit_password

# UAT Environment Configuration
UAT_VM_HOSTNAME=uat-vm-01.example.com
UAT_VM_USERNAME=ec2-user
UAT_VM_SSH_KEY=UAT_VM_SSH_KEY
UAT_DATASOURCE_URL=jdbc:postgresql://uat-db.example.com:5432/sampleapp
UAT_DB_USERNAME=sampleapp_uat_user
UAT_DB_PASSWORD=uat_password

# PROD Environment Configuration
PROD_VM_HOSTNAME=prod-vm-01.example.com
PROD_VM_USERNAME=ec2-user
PROD_VM_SSH_KEY=PROD_VM_SSH_KEY
PROD_DATASOURCE_URL=jdbc:postgresql://prod-db.example.com:5432/sampleapp
PROD_DB_USERNAME=sampleapp_prod_user
PROD_DB_PASSWORD=prod_password

# Service-specific VM Configuration (optional)
# Gateway Service
SIT_GATEWAY_SERVICE_VM_HOSTNAME=sit-gateway.example.com
SIT_GATEWAY_SERVICE_VM_USERNAME=admin
SIT_GATEWAY_SERVICE_VM_SSH_KEY=SIT_GATEWAY_SSH_KEY

# User Service
SIT_USER_SERVICE_VM_HOSTNAME=sit-user.example.com
SIT_USER_SERVICE_VM_USERNAME=admin
SIT_USER_SERVICE_VM_SSH_KEY=SIT_USER_SSH_KEY

# Calendar Service
SIT_CALENDAR_SERVICE_VM_HOSTNAME=sit-calendar.example.com
SIT_CALENDAR_SERVICE_VM_USERNAME=admin
SIT_CALENDAR_SERVICE_VM_SSH_KEY=SIT_CALENDAR_SSH_KEY

# Chat Service
SIT_CHAT_SERVICE_VM_HOSTNAME=sit-chat.example.com
SIT_CHAT_SERVICE_VM_USERNAME=admin
SIT_CHAT_SERVICE_VM_SSH_KEY=SIT_CHAT_SSH_KEY
